{
 "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": { "jsx": true, "experimentalObjectRestSpread": true},
    "jsx": true
  },
  "extends": [ "eslint:recommended", "plugin:react/recommended"],
  "env": {
     "es6": true,
     "browser": true,
     "node": true,
     "jquery": true,
  },
  "plugins": ["react"],
  "globals": {
      "env": false,
      "region": false,
      "api": false
  },
  rules: {
    "indent": ["error", 4],
    "semi": ["error", "always"],
    "no-unused-vars": ["warn"],
    no-console: ["off"],
  }
}
